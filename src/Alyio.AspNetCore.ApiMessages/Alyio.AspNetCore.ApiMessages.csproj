<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <Copyright>Copyright (c) 2016 ROY</Copyright>
    <AssemblyTitle>Alyio.AspNetCore.ApiMessages</AssemblyTitle>
    <VersionPrefix>3.0.0</VersionPrefix>
    <Authors>ROY</Authors>
    <TargetFrameworks>netcoreapp3.0</TargetFrameworks>
    <TreatWarningsAsErrors>true</TreatWarningsAsErrors>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <AssemblyName>Alyio.AspNetCore.ApiMessages</AssemblyName>
    <Description>Provides a mechanism to process unhandled exception occured during a HTTP context and write custom API result message.</Description>
    <PackageId>Alyio.AspNetCore.ApiMessages</PackageId>
    <PackageTags>AspNetCore;Api;Messages</PackageTags>
    <PackageReleaseNotes>
      1. Migrate to .NET Core App 3.0
      2. Replace Newtonsoft.Json with System.Text.Json
    </PackageReleaseNotes>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageProjectUrl>https://github.com/qqbuby/Alyio.AspNetCore.ApiMessages</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.githubusercontent.com/qqbuby/Alyio.AspNetCore.ApiMessages/dev/LICENSE</PackageLicenseUrl>
    <RepositoryType>git</RepositoryType>
    <RepositoryUrl>https://github.com/qqbuby/Alyio.AspNetCore.ApiMessages</RepositoryUrl>
    <GenerateAssemblyConfigurationAttribute>false</GenerateAssemblyConfigurationAttribute>
    <GenerateAssemblyCompanyAttribute>false</GenerateAssemblyCompanyAttribute>
    <GenerateAssemblyProductAttribute>false</GenerateAssemblyProductAttribute>
  </PropertyGroup>
  
  <ItemGroup>
    <FrameworkReference Include="Microsoft.AspNetCore.App" />
  </ItemGroup>
</Project>